{
    "name": "code23/voyager-forms",
    "description": "Ahoy! - A package to implement forms into Voyager",
    "keywords": [
        "laravel",
        "frontend",
        "voyager",
        "forms"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Barna Nagy (Code23)",
            "homepage": "https://www.code23.com",
            "email": "support@code23.com"
        }
    ],
    "require": {
        "code23/voyager-frontend": "^0.9",
        "tcg/voyager": "^1.6",
        "guzzlehttp/guzzle": "~6.0|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [],
        "psr-4": {
            "Pvtl\\VoyagerForms\\": "src/",
            "Pvtl\\VoyagerForms\\Database\\": "database/"
        }
    },
    "autoload-dev": {
        "classmap": [],
        "psr-4": {
            "Pvtl\\VoyagerForms\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pvtl\\VoyagerForms\\Providers\\VoyagerFormsServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/code23/voyager-frontend"
        },
        {
            "type": "vcs",
            "url": "https://github.com/code23/voyager-blog"
        },
        {
            "type": "vcs",
            "url": "https://github.com/code23/voyager-pages"
        }
    ],
    "scripts": {
        "test": "phpunit --colors=always"
    }
}
